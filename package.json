{
  "name": "obsidian-paste-cleaner",
  "version": "0.2.4",
  "description": "Obsidian plugin for removing unwanted strings from pasted content.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "check": "npm run format:check && npm run lint && npm run knip && typos && npm run check:types",
    "check:types": "concurrently \"npm:tsc:*\"",
    "tsc:root": "tsc --project ./ --noEmit -skipLibCheck",
    "tsc:src": "tsc --project ./src --noEmit -skipLibCheck",
    "tsc:tests": "tsc --project ./tests --noEmit -skipLibCheck",
    "tsc:tools": "tsc --project ./tools --noEmit -skipLibCheck",
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "release": "node tools/release.ts",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx}\"",
    "knip": "knip"
  },
  "keywords": [],
  "author": "fry69",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fry69/obsidian-paste-cleaner.git"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "builtin-modules": "^5.0.0",
    "chalk": "^5.3.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-obsidianmd": "^0.1.6",
    "globals": "^16.4.0",
    "knip": "^5.66.2",
    "prettier": "^3.6.2",
    "prompts": "^2.4.2",
    "semver-ts": "^1.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "obsidian": "latest"
  }
}
